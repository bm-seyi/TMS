{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "common",
    "ClientId": "58c9ae05-1d48-4f7e-ae04-a0926ae1cb8a"
  },
  "ReverseProxy": {
    "Routes": {
      "arcgis-route": {
        "ClusterId": "tms-api",
        "Match": {
          "Path": "/api/v{version}/secrets/arcgis",
          "Methods": [
            "GET"
          ]
        },
        "AuthorizationPolicy": "Default"
      }
    },
    "Clusters": {
      "tms-api": {
        "Destinations": {
          "tms": {
            "Address": "https://localhost:5001/"
          }
        }
      }
    }
  }
}